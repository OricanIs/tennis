<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.tennis.model.db.MatchResult" table="match_result" schema="tennis">
        <id name="id" column="id"/>
        <property name="matchId" column="match_id"/>
        <property name="challengeScore" column="challenge_score"/>
        <property name="defenderScore" column="defender_score"/>
        <property name="userId" column="user_id"/>
    </class>
</hibernate-mapping>